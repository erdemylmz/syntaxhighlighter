#include "PinConfig.h"
//8-bit function implementations void lcdInit(void); void lcdBusy(void); void peekBoo(void); void sendCommand(unsigned char command); void sendCharacter(unsigned char character); void sendString(char *string); void goLocation(uint8_t x, uint8_t y); void sendStringLocation(uint8_t x, uint8_t y, char *String); void sendIntegerLocation(uint8_t x, uint8_t y, int IntegerToDisplay, char NumberOfDigits); void buildCharacter(unsigned char loc, unsigned char *p); void initCustomCharacters(void);
//4-bit function implementation void lcdInitFour(void); void checkBusyFour(void); void sendCommandFour(unsigned char command); void sendCharacterFour(unsigned char character); void sendStringFour(char *string); void goLocationFour(uint8_t x, uint8_t y); void sendStringLocationFour(uint8_t x, uint8_t y, char *String); void lcdClearFour(void); void buildCharacterFour(unsigned char loc, unsigned char *p); void initCustomCharactersFour(void); void sendIntegerLocationFour(uint8_t x, uint8_t y, int IntegerToDisplay, char NumberOfDigits);
